{
  "name": "mixwith",
  "version": "0.1.1",
  "description": "A simple, powerful mixin applier for JavaScript classes",
  "main": "src/mixwith.js",
  "directories": {
    "test": "test"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "devDependencies": {
    "chai": "^3.4.1"
  },
  "scripts": {
    "test": "mocha --ui tdd test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinfagnani/mixwith.js.git"
  },
  "keywords": [
    "mixin",
    "mixins",
    "classes"
  ],
  "author": "Justin Fagnani <justin@fagnani.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/justinfagnani/mixwith.js/issues"
  },
  "homepage": "https://github.com/justinfagnani/mixwith.js#readme"
}
